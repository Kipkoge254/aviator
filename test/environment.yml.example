common: &common
  provider:     openstack
  auth_service:
    name:        identity
    host_uri:    http://devstack:5000
    request:     create_token
    api_version: v2
  
admin:
  <<: *common
  auth_credentials:
    username:   admin
    password:   mypassword
    tenantName: admin

non_admin:
  <<: *common
  auth_credentials:
    username:   nonadmin
    password:   mypassword
    tenantName: nonadminproject
